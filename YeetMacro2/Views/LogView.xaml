<?xml version="1.0" encoding="utf-8" ?>
<ContentView xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             xmlns:vm="clr-namespace:YeetMacro2.ViewModels"
             x:DataType="vm:LogViewModel"
             vm:ViewModelLocator.ViewModelType="{x:Type vm:LogViewModel}"
             xmlns:mct="http://schemas.microsoft.com/dotnet/2022/maui/toolkit"
             x:Class="YeetMacro2.Views.LogView">
    <mct:DockLayout>
        <Label Text="{Binding Message}" BackgroundColor="Blue" mct:DockLayout.DockPosition="Bottom" HeightRequest="20" />
        <Grid />
    </mct:DockLayout>

</ContentView>
